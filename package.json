{
  "name": "project-timer",
  "displayName": "Project Timer",
  "description": "Timer for VS Code",
  "publisher": "GoldSky",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
],
"repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repository.git"
}
,
  "main": "./out/extension.js",
"contributes": {
    "commands": [
        {
            "command": "project-timer.start",
            "title": "Start Timer"
        },
        {
            "command": "project-timer.pause",
            "title": "Pause Timer"
        },
        {
            "command": "project-timer.reset",
            "title": "Reset Timer"
        },
        {
            "command": "project-timer.showStats",
            "title": "Show Statistics"
        }
    ]
}
,
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
